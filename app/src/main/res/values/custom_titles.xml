<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 自定义标题栏所用布局文件属性-->
    <declare-styleable name="CustomTitles">
        <attr name="custom_titles_title_icon" format="reference"/><!--标题图片-->
        <attr name="custom_titles_title_text" format="string"/><!--标题文字-->
        <attr name="custom_titles_title_text_color" format="color"/><!--文字颜色-->
        <attr name="custom_titles_title_distance" format="dimension"/><!--标题图片和标题文字之间的距离-->
        <attr name="custom_titles_title_text_size" format="dimension"/><!--文字大小-->

        <!--搜索图标是否可见-->
        <attr name="custom_titles_search_visibility">
            <enum name="visible" value="0"/>
            <enum name="invisible" value="4"/>
            <enum name="gone" value="8"/>
        </attr>

        <!--菜单图标是否可见-->
        <attr name="custom_titles_menu_visibility" >
            <enum name="visible" value="0"/>
            <enum name="invisible" value="4"/>
            <enum name="gone" value="8" />
        </attr>
    </declare-styleable>
</resources>